    "def removeOccurrences(s, part):\n",
    "        stack = \"\"\n",
    "        part_size = len(part)\n",
    "        for i in s:\n",
    "            stack += i\n",
    "            if stack[len(stack)-part_size:] == part:\n",
    "                stack = stack[:len(stack)-part_size]\n",
    "        return stack"
   ]
  }
