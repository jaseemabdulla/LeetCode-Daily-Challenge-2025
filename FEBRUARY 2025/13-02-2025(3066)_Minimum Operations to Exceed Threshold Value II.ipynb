    "def minOperations(nums, k):\n",
    "        ans = 0\n",
    "        arr = SortedList(nums)\n",
    "        while len(arr) > 1 and k > arr[0]:\n",
    "            ans += 1\n",
    "            arr.add((arr[0] * 2) + arr[1])\n",
    "            arr.discard(arr[1])\n",
    "            arr.discard(arr[0])\n",
    "        return ans"
   ]
