   "source": [
    "def partitionArray(nums, k):\n",
    "        nums.sort()\n",
    "        cur, ans, n = 0, 1, len(nums)\n",
    "        for i in range(n):\n",
    "            if k < nums[i] - nums[cur]:\n",
    "                cur = i\n",
    "                ans += 1\n",
    "        return ans"
   ]
  }
